{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "number-extracter",
  "version": "0.1.0",
  "identifier": "com.number-extracter.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "跟踪单号提取",
        "minHeight": 768,
        "minWidth": 1200
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "./rtens"
    ]
  }
}
