    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@vitejs/plugin-vue": "^5.2.1",
    "daisyui": "^5.0.0-beta.6",
    "npm-run-all2": "^7.0.2",
